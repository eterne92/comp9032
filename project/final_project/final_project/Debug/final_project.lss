                                 ;----------------------------------
                                 ;FUNCTIONS RELATED TO KEYPAD
                                 ;GET_KEY
                                 ;KEY_ASCII
                                 ;----------------------------------
                                 ;KEYPAD SHOULD BE CONNECTED AS BELOW
                                 ;KEYPAD -> PORTL
                                 ;COLUMS -> 4:7
                                 ;ROWS -> 0:3
                                 ;PORT DIRECTIONS SHOULD BE SET ON RESET
                                 ;----------------------------------
                                 .EQU ROWMASK = 0X0F
                                 .EQU COLMASK = 0B11101111	;START FROM C0
                                 
                                 ;FUNCTION TO GET KEYVALUE FROM KEYPAD, RETURN ROW:COL ON R24:R25
                                 GET_KEY:
000000 930f                      	PUSH R16				;R16 AS RMASK
000001 931f                      	PUSH R17				;R17 AS ROW
000002 932f                      	PUSH R18				;R18 AS COL
000003 933f                      	PUSH R19				;R19 AS TEMP1
000004 934f                      	PUSH R20				;R20 AS TEMP2
000005 935f                      	PUSH R21				;R21 AS CMASK
                                 	SET_CMASK:
000006 ee5f                      		LDI R21, COLMASK
000007 9350 010b                 		STORE KEY_PORT, R21	;SET COLMASK TO 0X11101111
000009 2722                      		CLR R18				;SET COL TO 0
                                 	CLOOP:
00000a e001                      		LDI R16, 0X01		;START FROM ROW 0
00000b 2711                      		CLR R17				;SET ROW TO 0
                                 	RLOOP:
00000c 9130 0109                 		LOAD R19, PINL		;GET VALUE FROM KEYPAD
00000e 2f43                      		MOV R20, R19
00000f 704f                      		ANDI R20, ROWMASK	;CLR COL INPUT
000010 2340                      		AND R20, R16
000011 f409                      		BRNE NEXT_ROW
000012 c00d                      		RJMP DEBOUNCING_LOOP
                                 	NEXT_ROW:
000013 0f00                      		LSL R16				;LEFTSHIFT RMASK
000014 9513                      		INC R17				;ROW = ROW + 1
000015 3014                      		CPI R17, 4			;IF ROW == 4
000016 f009                      		BREQ NEXT_COL		;GOTO NEXT COL
000017 cff4                      		RJMP RLOOP			;ELSE GOTO NEXT ROW
                                 	NEXT_COL:
000018 0f55                      		LSL R21				;LEFTSHIFT CMASK
000019 9523                      		INC R18				;COL = COL + 1
00001a 3024                      		CPI R18, 4			;IF COL == 4
00001b f351                      		BREQ SET_CMASK		;STARTOVER AGAIN
00001c 9553                      		INC R21
00001d 9350 010b                 		STORE KEY_PORT, R21		;SET CMASK
00001f cfea                      		RJMP CLOOP			;GOTO NEXT COL
                                 
                                 	DEBOUNCING_LOOP:		;SOLVE DEBOUNCING
000020 9130 0109                 		LOAD R19, PINL
000022 703f                      		ANDI R19, ROWMASK
000023 303f                      		CPI R19, 0X0F
000024 f7d9                      		BRNE DEBOUNCING_LOOP
000025 2f81                      	MOV R24, R17			;R24 AS ROW
000026 2f92                      	MOV R25, R18			;R25 AS COL
                                 
000027 915f                      	POP R21
000028 914f                      	POP R20
000029 913f                      	POP R19
00002a 912f                      	POP R18
00002b 911f                      	POP R17
00002c 910f                      	POP R16
00002d 9508                      	RET
                                 
                                 ;GET KEY_ASCII VALUE FROM INPUT AS R24->ROW, R25->COL, RETURN ASCII VALUE AS R23
                                 KEY_ASCII:
00002e 938f                      	PUSH R24				;R24 AS ROW
00002f 939f                      	PUSH R25				;R25 AS COL
000030 930f                      	PUSH R16				;R16 AS TEMP
000031 920f                      	PUSH R0
000032 921f                      	PUSH R1					;MUL WILL BE USED
                                 
000033 dfcc                      	RCALL GET_KEY
000034 3093                      	CPI R25, 3				;IF COL == 3
000035 f049                      	BREQ ASCII_LETTER
000036 3083                      	CPI R24, 3				;IF ROW == 3
000037 f059                      	BREQ ASCII_SYMBOL
000038 e003                      	LDI R16, 3				;TEMP = 3 * ROW + COL
000039 9f80                      	MUL R24, R16			;
00003a 2d00                      	MOV R16, R0
00003b 0f09                      	ADD R16, R25 
00003c 5c0f                      	SUBI R16, -'1'			;GET ASCII VALUE
00003d 2f70                      	MOV R23, R16
00003e c00e                      	RJMP RETURN_KEY_ASCII
                                 
                                 	ASCII_LETTER:
00003f e401                      		LDI R16, 'A'			;TEMP = 'A'
000040 0f08                      		ADD R16, R24			;TEMP += ROW
000041 2f70                      		MOV R23, R16
000042 c00a                      		RJMP RETURN_KEY_ASCII
                                 
                                 	ASCII_SYMBOL:
000043 3090                      		CPI R25, 0				;'*'
000044 f021                      		BREQ ASCII_STAR
000045 3091                      		CPI R25, 1				;'0'
000046 f021                      		BREQ ASCII_ZERO
000047 e273                      		LDI R23, '#'			;'#'
000048 c004                      		RJMP RETURN_KEY_ASCII
                                 
                                 	ASCII_STAR:
000049 e27a                      		LDI R23, '*'
00004a c002                      		RJMP RETURN_KEY_ASCII
                                 
                                 	ASCII_ZERO:
00004b e370                      		LDI R23, '0'
00004c c000                      		RJMP RETURN_KEY_ASCII
                                 
                                 RETURN_KEY_ASCII:
00004d 901f                      	POP R1
00004e 900f                      	POP R0
00004f 910f                      	POP R16
000050 919f                      	POP R25
000051 918f                      	POP R24
