                                 	BUSY_LOOP:
000058 d078                      		RCALL WAIT_1MS		;GET SOME DELAY
000059 9a16                      		SBI LCD_CTRL_PORT, LCD_E
00005a d076                      		RCALL WAIT_1MS		;GET SOME DELAY
00005b e00f                      		LDI R16, PINF
00005c 9816                      		CBI LCD_CTRL_PORT, LCD_E
00005d fd07                      		SBRC R16, 7			;IF BF IS CLEAR, END BUSY LOOP
00005e cff9                      		RJMP BUSY_LOOP		;ELSE LOOP UNTIL IT'S SET
00005f 2700                      	CLR R16
000060 b902                      	OUT LCD_CTRL_PORT, R16
000061 ef0f                      	SER R16
000062 bb00                      	OUT DDRF, R16
000063 910f                      	POP R16
000064 9508                      	RET
                                 
