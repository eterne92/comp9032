                                 ;FUNCTION TO WAIT FOR 1MS
                                 WAIT_1MS:
0000d1 938f                      	PUSH R24
0000d2 939f                      	PUSH R25
                                 
0000d3 e084                      	LDI R24, LOW(4100)
0000d4 e190                      	LDI R25, HIGH(4100)		;A LITTLE MORE THAN 1MS
