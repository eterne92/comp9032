                                 .include "drone.asm"
                                 
                                 ;R20-> ROW, R21-> COL, R18 -> DIRECTION
                                 ;Z POINT TO LAST POSITION
                                 ;NEW_HEIGHT -> R23 AS RETURN
                                 ;IF REACH END, RETURN -1(WHICH MIGHT NEVER HAPPEN)
                                 FLY_TO_NEXT_POS:    
0000e0 3020                          CPI R18, 0      ;R18 -> SET AS FLY RIGHT, CLEAR AS FLY LEFT
0000e1 f041                          BREQ GO_LEFT
0000e2 335f                          CPI R21, BORDER_X
0000e3 f059                          BREQ GO_DOWN
0000e4 9631                          ADIW Z, 1
0000e5 5f5f                          SUBI R21, -1
                                 
                                     CHECK_HEIGHT:
0000e6 9174                              LPM R23, Z
0000e7 9508                              RET
                                 
                                     GO_END:
0000e8 ef7f                              LDI R23, -1
0000e9 9508                              RET
                                 
                                     GO_LEFT:
