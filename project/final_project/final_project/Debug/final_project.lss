000068 d068                      	RCALL WAIT_1MS
000069 9a16                      	SBI LCD_CTRL_PORT, LCD_E		;ENABLE
00006a d066                      	RCALL WAIT_1MS
00006b 9816                      	CBI LCD_CTRL_PORT, LCD_E
00006c d064                      	RCALL WAIT_1MS
00006d 9508                      	RET		
                                 
